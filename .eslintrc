{
	"extends": ["react-app", "prettier"],
	"plugins": ["react", "prettier"],
	"rules": {
		"prettier/prettier": [
			"warn",
			{
				"arrowParens": "always",
				"semi": true,
				"trailingComma": "none",
				"endOfLine": "lf",
				"tabWidth": 4,
				"useTabs": true
			}
		],
		"no-console": "warn"
	}
}
